; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Try Godot"
run/main_scene="res://scenes/entrance.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false

[editor_plugins]

enabled=PackedStringArray("res://addons/gut/plugin.cfg")

[internationalization]

locale/translation_remaps={}
locale/translations=PackedStringArray("res://localizations/zh_TW.po")
locale/translations_pot_files=PackedStringArray("res://scenes/entrance.tscn", "res://scenes/player_room.tscn")
locale/locale_filter_mode=1
locale/language_filter=["zh"]
locale/country_filter=["TW"]

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
